    "    for row in rows:\n",
    "        print(f\"\"\"\n",
    "        Character: {row[1]}\n",
    "        Total Pulls: {row[2]}\n",
    "        # Pulled: {row[3]}\n",
    "        % of Total Pulls: {row[4]:.2f}%\n",
    "        Estimated Revenue (in millions): ${row[5]:.2f}\n",
    "        Planet/Group: {row[6]}\n",
    "        Time Between Story and Banner (days): {row[7]}\n",
